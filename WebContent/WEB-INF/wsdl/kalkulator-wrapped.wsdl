	</wsdl:message>
	<wsdl:message name="divResponse">
		<wsdl:part name="parameters" element="tns:divResponse"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="avgRequest">
		<wsdl:part name="parameters" element="tns:avg"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="avgResponse">
		<wsdl:part name="parameters" element="tns:avgResponse"></wsdl:part>
	</wsdl:message>

	<wsdl:portType name="kalkulator">
		<wsdl:operation name="add">
			<wsdl:input message="tns:addRequest" />
			<wsdl:output message="tns:addResponse" />
		</wsdl:operation>
		<wsdl:operation name="sub">
			<wsdl:input message="tns:subRequest"></wsdl:input>
			<wsdl:output message="tns:subResponse"></wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="mul">
			<wsdl:input message="tns:mulRequest"></wsdl:input>
			<wsdl:output message="tns:mulResponse"></wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="div">
			<wsdl:input message="tns:divRequest"></wsdl:input>
			<wsdl:output message="tns:divResponse"></wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="avg">
			<wsdl:input message="tns:avgRequest"></wsdl:input>
			<wsdl:output message="tns:avgResponse"></wsdl:output>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="kalkulatorSOAP" type="tns:kalkulator">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="add">
			<soap:operation soapAction="http://www.example.org/kalkulator/add" />
			<wsdl:input>
