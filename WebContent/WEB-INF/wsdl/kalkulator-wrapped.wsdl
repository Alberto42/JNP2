				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="IntResult">
				<xsd:sequence>
					<xsd:element name="result" type="xsd:int" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="DoubleResult">
				<xsd:sequence>
					<xsd:element name="result" type="xsd:double" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="DivResult">
				<xsd:sequence>
					<xsd:element name="quotient" type="xsd:int" />
